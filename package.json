{
  "name": "@kareniel/www",
  "version": "0.0.0",
  "description": "kareniel's personal website",
  "main": "src",
  "scripts": {
    "start": "budo src/index.js:bundle.js --dir public --live",
    "build": "mkdir -p dist && cp public/* dist/ && browserify -p tinyify src/index.js > dist/bundle.js",
    "test": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kareniel/www.git"
  },
  "keywords": [
    "www"
  ],
  "author": "kareniel <hello@jonathandupre.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kareniel/www/issues"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "homepage": "https://github.com/kareniel/www#readme",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "babelify": "^10.0.0",
    "budo": "^11.5.0",
    "standard": "^12.0.1",
    "tinyify": "^2.5.0",
    "uglify-js": "^3.4.9",
    "uglifyify": "^5.0.1"
  },
  "dependencies": {
    "ansi-escapes": "^3.1.0",
    "babel-polyfill": "^6.26.0",
    "xterm": "^3.8.0"
  }
}
